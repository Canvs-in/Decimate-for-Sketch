{
  "name": "decimate",
  "description": "Round Fractional font sizes",
  "version": "1.0.1",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "decimate",
    "manifest": "src/manifest.json",
    "main": "decimate.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Depro20 <roy@canvs.in>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Canvs-in/Decimate-for-Sketch.git"
  }
}
